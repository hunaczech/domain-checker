{
    "name": "lkshnk/domain-checker",
    "description": "CLI tool to check domain availability via WHOIS",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "amphp/socket": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/console"],
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
